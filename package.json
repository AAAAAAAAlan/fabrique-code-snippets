{
	"name": "fabrique-code-snippets",
	"displayName": "Fabrique Code Snippets",
	"description": "Code snippets for custom vue directives that we use.",
	"version": "1.0.0",
	"publisher": "Alan",
	"engines": {
		"vscode": "^1.28.0"
	},
	"keywords": [
    "Vue",
    "Vue Snippets"
  ],
	"extensionDependencies": [
    "octref.vetur"
  ],
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "vue-html",
				"path": "./snippets/border.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/bounds.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/colors.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/cursor.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/flexGrid.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/sizes.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/text.json"
			}
		]
	}
}
